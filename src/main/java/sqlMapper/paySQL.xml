<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ace.pay">
	<insert id="addPay" parameterType="Map">
		insert into
		pay
		values(
		#{com_idx},
		sysdate,
		#{price_sort},
		#{usernum}
		)
	</insert>
</mapper>